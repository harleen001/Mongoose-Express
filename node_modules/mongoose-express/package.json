{
  "name": "mongoose-express",
  "version": "0.0.1",
  "description": "An easy intergration for express routes that connect to mongodb using mongoose",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "mocha",
    "dev": "tsc-watch --onSuccess \"node --inspect=9229 ./dist/index.js\"",
    "coverage": "nyc npm run test",
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc --watch false -d",
    "lint": "eslint --ignore-path .gitignore .",
    "tsc": "tsc"
  },
  "files": [
    "dist"
  ],
  "author": "Relief.Melone",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^6.17.2",
    "@sentry/tracing": "^6.17.2",
    "@types/chai": "latest",
    "@types/express": "latest",
    "@types/mocha": "latest",
    "@types/node": "latest",
    "merge-anything": "^4.0.5",
    "mongoose": "^6.1.8",
    "query-bouncer-mongoose-plugin": "2.0.1-1",
    "sinon": "^12.0.1",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/sinon": "^10.0.8",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "latest",
    "eslint": "latest",
    "eslint-import-resolver-alias": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-import": "latest",
    "mocha": "latest",
    "mongodb-memory-server": "^8.2.0",
    "nyc": "latest",
    "source-map-support": "latest",
    "tsc-watch": "latest"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/rm-npm-packages/mongoose-express.git"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "express",
    "nodejs",
    "node",
    "typescript"
  ],
  "bugs": {
    "url": "https://gitlab.com/rm-npm-packages/mongoose-express/issues"
  },
  "homepage": "https://gitlab.com/rm-npm-packages/mongoose-express#readme"
}
